{
  "routes": [
    {
      "src": "/(.*).js",
      "headers": { "content-type": "text/javascript" },
      "dest": "/$1.js"
    },
    {
      "src": "/(.*).css",
      "headers": { "content-type": "text/css" },
      "dest": "/$1.css"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
